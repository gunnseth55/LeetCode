class Solution {
  public:
    int cycleStart(Node* head) {
        // code here
        Node *slow=head;
        Node *fast=head;
        while(fast && fast->next){
            slow=slow->next;
            fast=fast->next->next;
            if(slow==fast){
                slow=head;
                while(slow!=fast){
                    slow=slow->next;
                    fast=fast->next;
                }
                
                if(slow==fast)return slow->data;
            }
        }
        return -1;
    }
};
